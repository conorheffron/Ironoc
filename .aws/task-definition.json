{
    "family": "ironoc",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "ironoc",
            "image": "public.ecr.aws/y3a1j7s4/conorheffron/ironoc",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "ironoc-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "entryPoint": [
                "sh",
                "-c"
            ],
            "command": [
                "/bin/sh -c /run.sh"
            ]
        }
    ],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}