{
    "containerDefinitions": [
        {
            "name": "ironoc",
            "image": "public.ecr.aws/y3a1j7s4/conorheffron/ironoc:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "ironoc-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "entryPoint": [
                "java",
                "-jar",
                "app.war"
            ],
            "essential": true
        }
    ],
    "family": "ironoc",
    "executionRoleArn": "arn:aws:iam::345594590074:role/ecsTaskExecutionRole",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}